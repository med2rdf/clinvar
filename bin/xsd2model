#!/usr/bin/env ruby

require 'bundler/setup'
require 'clinvar/rdf'

input = ARGV[0]
output = File.expand_path('../../lib/clinvar/rdf/model.rb', __FILE__)

raise('File not found') unless File.exist?(input)

File.open(output, 'w') do |f|
  f << ClinVar::XSD.open(input) { |xsd| xsd.to_ruby }
  STDERR.puts "wrote to #{output}"
end
